{
  "name": "snifftsx",
  "version": "1.0.0",
  "description": "A tool for detecting code smells in React with TypeScript projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --outDir dist",
    "start": "node dist/lib/index.js test/components",
    "start:dev": "tsx lib/index.ts test/components",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maykongsn/snifftsx.git"
  },
  "keywords": [
    "react",
    "typescript",
    "smells"
  ],
  "author": "Maykon Nunes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maykongsn/snifftsx/issues"
  },
  "homepage": "https://github.com/maykongsn/snifftsx#readme",
  "dependencies": {
    "@babel/parser": "7.25.3",
    "@babel/traverse": "7.25.3",
    "csv-parse": "5.5.6",
    "csv-stringify": "6.5.1",
    "reactsniffer": "1.0.19"
  },
  "devDependencies": {
    "@types/babel__traverse": "7.20.6",
    "@types/babel-types": "7.0.15",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  }
}
